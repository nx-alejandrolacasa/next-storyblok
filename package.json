{
  "name": "next-storyblok",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:https": "next dev --turbo --experimental-https --port 3010",
    "build": "next build",
    "start": "next start",
    "lint": "biome check .",
    "lint:fix": "biome check --write --unsafe .",
    "test": "vitest run",
    "test:watch": "vitest",
    "blok:pull-components": "storyblok pull-components --space=$(grep STORYBLOK_SPACE_DEV .env | cut -d '=' -f2) --path=./types/",
    "blok:generate-types": "storyblok generate-typescript-typedefs --sourceFilePaths=./types/components.$(grep STORYBLOK_SPACE_DEV .env | cut -d '=' -f2).json --target=./types/sb-types.d.ts",
    "blok:types": "run-s blok:pull-components blok:generate-types",
    "blok:sync": "npx storyblok sync --type components --source $(grep STORYBLOK_SPACE_DEV .env | cut -d '=' -f2) --target $(grep STORYBLOK_SPACE_MAIN .env | cut -d '=' -f2)"
  },
  "dependencies": {
    "@conform-to/react": "~1.7.0",
    "@conform-to/zod": "~1.7.0",
    "@hookform/resolvers": "~5.1.1",
    "@radix-ui/react-icons": "~1.3.2",
    "@radix-ui/react-label": "~2.1.7",
    "@radix-ui/react-slot": "~1.2.3",
    "@storyblok/react": "~5.0.0",
    "@storyblok/richtext": "~3.3.0",
    "class-variance-authority": "~0.7.1",
    "colord": "~2.9.3",
    "isomorphic-dompurify": "~2.25.0",
    "next": "~15.3.3",
    "next-intl": "~4.1.0",
    "react": "~19.1.0",
    "react-dom": "~19.1.0",
    "sharp": "~0.34.2",
    "tailwind-merge": "~3.3.1",
    "zod": "~3.25.67"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@tailwindcss/postcss": "4.1.10",
    "@types/node": "~24.0.3",
    "@types/react": "~19.1.8",
    "@types/react-dom": "~19.1.6",
    "clsx": "~2.1.1",
    "npm-run-all": "~4.1.5",
    "postcss": "~8.5.6",
    "storyblok": "~3.36.1",
    "storyblok-generate-ts": "~2.2.0",
    "tailwindcss": "4.1.10",
    "typescript": "~5.8.3",
    "vite-tsconfig-paths": "~5.1.4",
    "vitest": "~3.2.3"
  }
}
