{
  "name": "next-storyblok",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "sb:pull-components": "storyblok pull-components --space=185369 --path=./types/",
    "sb:generate-types": "storyblok-generate-ts source=./types/components.185369.json target=./types/sb-types",
    "sb:eslint-types": "eslint --fix ./types/sb-types.d.ts",
    "sb:prettier-types": "prettier --write ./types/sb-types.d.ts",
    "sb:types": "run-s sb:pull-components sb:generate-types sb:eslint-types sb:prettier-types"
  },
  "dependencies": {
    "@storyblok/react": "~2.0.18",
    "autoprefixer": "~10.4.14",
    "next": "~13.3.4",
    "react": "~18.2.0",
    "react-dom": "~18.2.0"
  },
  "devDependencies": {
    "@types/node": "~18.16.3",
    "@types/react": "~18.2.0",
    "@types/react-dom": "~18.2.1",
    "clsx": "~1.2.1",
    "eslint": "~8.39.0",
    "eslint-config-next": "~13.3.4",
    "eslint-plugin-unused-imports": "~2.0.0",
    "npm-run-all": "~4.1.5",
    "postcss": "~8.4.23",
    "prettier": "~2.8.8",
    "storyblok": "~3.21.0",
    "storyblok-generate-ts": "~1.13.2",
    "tailwindcss": "~3.3.2",
    "typescript": "~5.0.4",
    "vitest": "~0.30.1"
  }
}
