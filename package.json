{
  "name": "next-storyblok",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "sb:pull-components": "storyblok pull-components --space=185369 --path=./types/",
    "sb:generate-types": "storyblok-generate-ts source=./types/components.185369.json target=./types/sb-types",
    "sb:eslint-types": "eslint --fix ./types/sb-types.d.ts",
    "sb:prettier-types": "prettier --write ./types/sb-types.d.ts",
    "sb:types": "run-s sb:pull-components sb:generate-types sb:eslint-types sb:prettier-types"
  },
  "dependencies": {
    "@storyblok/react": "~2.4.7",
    "colord": "~2.9.3",
    "fast-xml-parser": "~4.3.2",
    "isomorphic-dompurify": "~1.9.0",
    "next": "~14.0.2",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "swr": "~2.2.4"
  },
  "devDependencies": {
    "@types/node": "~20.9.0",
    "@types/react": "~18.2.37",
    "@types/react-dom": "~18.2.15",
    "autoprefixer": "~10.4.16",
    "clsx": "~2.0.0",
    "eslint": "~8.53.0",
    "eslint-config-next": "~14.0.2",
    "eslint-plugin-tailwindcss": "~3.13.0",
    "eslint-plugin-unused-imports": "~3.0.0",
    "npm-run-all": "~4.1.5",
    "postcss": "~8.4.31",
    "prettier": "~3.1.0",
    "prettier-plugin-tailwindcss": "~0.5.7",
    "storyblok": "~3.25.3",
    "storyblok-generate-ts": "~2.0.1",
    "tailwindcss": "~3.3.5",
    "typescript": "~5.2.2",
    "vitest": "~0.34.6",
    "vitest-tsconfig-paths": "~3.4.1"
  }
}
